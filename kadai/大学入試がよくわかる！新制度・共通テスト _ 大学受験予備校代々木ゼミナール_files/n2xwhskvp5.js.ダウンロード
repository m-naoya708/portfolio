
// goo-pix
(function(){
  var script = document.createElement('script');
  script.charset = 'utf-8';
  script.async = true;
  script.defer = true;
  script.src = '//bwb101.goo.ne.jp/pix.js?id=n2xwhskvp5&url=' + escape(document.location) + '&ref=' + escape(document.referrer);
  (document.head || document.getElementsByTagName('head')[0]).appendChild(script);
})();


